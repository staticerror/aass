							<option>0</option>
							<flag>wxLEFT|wxRIGHT|wxTOP</flag>
							<border>10</border>
							<object class="wxStaticText" name="keyword_static">
								<font>
									<family>default</family>
									<style>normal</style>
									<weight>bold</weight>
									<underlined>0</underlined>
								</font>
								<label>Enter Keyword:</label>
							</object>
						</object>
