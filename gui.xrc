									<item>Answers</item>
									<item>Microposts(Twitter)</item>
									<item>Wikis</item>
								</content>
							</object>
						</object>
						<object class="sizeritem">
							<option>0</option>
							<flag>wxEXPAND|wxTOP</flag>
							<border>5</border>
							<object class="wxBoxSizer">
								<orient>wxHORIZONTAL</orient>
								<object class="sizeritem">
									<option>1</option>
									<flag>wxALL|wxEXPAND|wxTOP</flag>
									<border>10</border>
